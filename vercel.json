{
  "version": 2,
  "devCommand": "next dev",
  "framework": "nextjs",
  "installCommand": "npm install @supabase/supabase-js@2.50.2 --save-exact && npm install",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "axle-cursor": true,
      "restore-v113": true
    },
    "productionBranch": "main"
  }
}
