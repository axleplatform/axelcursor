{
  "version": 2,
  "installCommand": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
  "buildCommand": "pnpm run build",
  "devCommand": "next dev",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@next_public_supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@next_public_supabase_anon_key"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "axle-cursor": true,
      "restore-v113": true
    },
    "productionBranch": "main"
  }
} 